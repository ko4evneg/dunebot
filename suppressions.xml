<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks="FileLengthCheck" files=".*Test\.java"/>
    <suppress checks="LineLength" files=".*Test\.java"/>
    <suppress checks="MethodLength" files=".*Test\.java|ExternalMessageFactory.*\.java"/>
    <suppress checks="EmptyCatchBlock" files=".*Test\.java"/>
    <suppress checks="HideUtilityClassConstructor" files="DunebotApplication\.java|LogId\.java"/>
</suppressions>
